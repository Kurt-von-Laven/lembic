<% variable = @var %>
<div id="array_output">  <%= variable.name.split('[i]')[0] %> </div>
<%  truncated_array = variable.expression_string.split(/\[\s*i\s*\|\s*/)[1][0..-2]
	comma_separated_array = truncated_array.split(/\s*,\s*/)
	num = comma_separated_array.size()
%>
<div>
	<table>
	<th> index </th>
	<% for k in 1..10 %>
			<th> <%= k %> </th>
	<% end %>
<% i = 0
	while i < num %>
		<tr>
		<td id ="col_header"> <%= i %> </td>
		<% for j in 1..10 %>
			<td>
			<%=comma_separated_array[i+j] %>
			</td>
		<% end %>
		</tr>
<% i+=10;
	end 
%>
	
	</table>
</div>